<module id="fileupload" excludes="android,gwt" type="integration"
	package="org.restlet.ext.fileupload">
	<name>Restlet Extension - FileUpload</name>
	<description>
		Integration with Apache FileUpload.
	</description>
	<distributions>
		<distribution id="classic" />
		<distribution id="maven" />
	</distributions>

	<dependencies>
		<dependency type="library" id="servlet" maven-scope="provided" />
		<dependency type="library" id="fileupload" primary="true" />
		<dependency type="module" id="core" />
	</dependencies>
</module>
