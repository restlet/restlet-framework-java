<library id="raml" symbolicName="org.raml">
	<name>Raml parser</name>
	<description>Java implementation of the raml parser</description>
	<version>0.8</version>
	<release>12</release>
	<homeUri>http://raml.org</homeUri>
	<downloadUri>http://raml.org</downloadUri>
	<provider>Mule Soft</provider>

	<package id="raml" name="org.raml">
	    <groupId>org.raml</groupId>
	    <artifactId>raml-parser</artifactId>
	    <version>0.8.12</version>
